<div class="listaartistas">
  <div class="arregloartistas">
    {{#each items as |i|}}
      <div class="col-md-4">
        <img class= "item-image" src="{{ i.images.2.url }}">
          <div>
            <p>{{i.name}}</p>
            <a href="/artist/{{i.id}}">View albums</a>
          </div>
        </div>
    {{/each}}
  </div>
</div>